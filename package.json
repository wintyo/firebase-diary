{
  "name": "electron-diary",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=12.15.x"
  },
  "browserslist": [
    "last 2 version",
    "IE 11"
  ],
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=tsconfig-for-node.json webpack-dev-server --config webpack.config.development.ts",
    "start:demo": "cross-env APP_ENV=demo yarn start",
    "start:prod": "cross-env APP_ENV=prod yarn start",
    "build": "rimraf dist; cross-env TS_NODE_PROJECT=tsconfig-for-node.json webpack --config webpack.config.production.ts",
    "build:demo": "cross-env APP_ENV=demo yarn build",
    "build:prod": "cross-env APP_ENV=prod yarn build",
    "deploy:demo": "cp database.demo.rules.json database.rules.json; firebase use demo; firebase deploy",
    "deploy:prod": "cp database.prod.rules.json database.rules.json; firebase use prod; firebase deploy"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.6",
    "date-fns": "^2.14.0",
    "firebase": "^7.16.0",
    "lodash-es": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1",
    "reset-css": "^5.0.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/firebase": "^3.2.1",
    "@types/lodash-es": "^4.17.3",
    "@types/react-dom": "^16.9.8",
    "@types/webpack": "^4.41.18",
    "autoprefixer": "^9.8.4",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "firebase-tools": "^8.5.0",
    "html-webpack-plugin": "^4.3.0",
    "ip": "^1.1.5",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "postcss": "^7.0.32",
    "postcss-loader": "^3.0.0",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "react-hot-loader": "^4.12.21",
    "rimraf": "^3.0.2",
